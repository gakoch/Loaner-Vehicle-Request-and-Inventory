        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    //Type appropriate comment here, and begin script below
    var dateNeeded = g_form.getValue('date_needed');
    var dateToReturnVehicle = g_form.getValue('date_to_return_vehicle');

    //if date is before date needed trigger error message
    if (dateToReturnVehicle < dateNeeded) {
        g_form.addErrorMessage('Date to return vehicle should not be before the Date needed');
        g_form.clearValue('date_to_return_vehicle');
        return false; // Prevent form submission
    }
    return true; // Allow form submission
}]]></script>
